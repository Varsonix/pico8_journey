-- primary game draw loop
function draw_game()
	cls(0)
	star_field() --generate stars
	
	-- handle blinking invuln
	if game.lives > 0 then
		if game.invuln <= 0 then
			draw_ship(ship)
		else
			-- during invuln
			if sin(t/5) < 0.1 then
				draw_ship(ship)
			end
		end
	end
	
	
	draw_objects(bullets) -- bullets
	draw_enemies(enemies) -- enemies
	
	muzzle_flash(ship) --  logic
	
	-- drawing particles
	draw_smolwaves()
	particle_system()
	shockwave_system()



	
	-- draw score
	print("score: "..game.score, 40, 3, 12)
	
	-- draw lives for loop
	for i=1,game.lives do
		if game.lives >= i then
			spr(14,i*9-8,2)
		else
			spr(13,i*9-8,2)
		end
	end
	
	-- draw bombs
	for i=1,game.bombs do
		spr(29,127-i*9, 2)
	end
	
	-- debugging printers
--	print(#particles, 3, 10)
--	print(#shockwaves, 3, 16)
end

function draw_start()
	cls(1)
	star_field() --generate stars
	print("star shooter", 40, 50, 12)
	print("press ❎ to start", 30, 80, blink())
end

function draw_over()
	draw_game()
	star_field() --generate stars
	print("game over", 45, 50, 8)
	print("press ❎ to restart", 27, 80, blink())
end

function draw_victory()
	draw_game()
	star_field() --generate stars
	print("a winner is you!", 34, 50, 12)
	print("press ❎ to restart", 27, 80, blink())
end


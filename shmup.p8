	end
	if page > 7 then
		col = 9
	end
	if page > 10 then
		col = 8
	end
	if page > 12 then
		col = 2
	end
	if page > 15 then
		col = 5
	end
	
	return col
end

function blue_page(page)
	local col = 7
	
	if page > 5 then
		col = 6
	end
	if page > 7 then
		col = 12
	end
	if page > 10 then
		col = 13
	end
	if page > 12 then
		col = 1
	end
	if page > 15 then
		col = 1
	end
	
	return col
end


function blink()
	local blink_ani = {
		5,5,5,5,5,5,5,6,6,
		7,7,6,6,5,5
	}
	if blink_timer > #blink_ani then 
		blink_timer = 1
	end
	
	return blink_ani[blink_timer]
end


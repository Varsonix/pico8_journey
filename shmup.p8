function _init()
	cls(0)
	start_screen()
	blink_timer = 0
	t = 0
	lockout = 0
end

function _update()
	t += 1
	blink_timer += 1
	if game.mode == "game" then
		update_game()
	elseif game.mode == "start" then
		update_start()
	elseif game.mode == "wavetext" then
		update_wavetext()
	elseif game.mode == "over" then
		update_over()
	elseif game.mode == "victory" then
		update_victory()
	end
end

function _draw()

	if game.mode == "game" then
		draw_game()
	elseif game.mode == "start" then
		draw_start()
	elseif game.mode == "wavetext" then
		draw_wavetext()
	elseif game.mode == "over" then
		draw_over()
	elseif game.mode == "victory" then
		draw_victory()
	end
end

function animate_thrusters()
	ship.boostspr += 1
	if ship.boostspr > 8 then
		ship.boostspr = 4
	end
end

function start_screen()
	generate_stars()
	game = {mode = "start"}
	music(7, 500)
end

function start_game()
	cls(1)
	-- ship object (player)
	ship = make_spr()
	ship.spr=2
	ship.x=60
	ship.y=100
	ship.muzzle=0
	ship.spdx=0
	ship.spdy=0
	ship.boostspr=4

	timer=0
	-- bullets holding bay
	bullets = {}
	
	-- enemies holding bay
	enemies = {}
	
	-- particle system holding bay
	explosions = {}
	shockwaves = {}
	smol_waves = {}
	particles = {}
	
	t=0

	-- general game variables
	game = {
		mode = "wavetext",
		score = flr(rnd(128)),
		lives = 1,
		invuln = 0,
		bombs = 3,
		wave = 0,
		wavetime = 80
	}
	
	next_wave()
end



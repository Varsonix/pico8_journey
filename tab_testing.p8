__lua__
-- main initilization

function _init()
	cls(0) -- for dark blue
	sfx(0)

	-- jordan object
	jordan = {
		name="jordan",
		posx=64,
		posy=64,
		age=15,
		sp=1
	}
	
	-- mark object
	mark = {
		name="mark",
		posx=72,
		posy=64,
		age=32,
		sp=2
	}
end
-->8
-- main game loop
function _update()
	jordan.posy+=1
	mark.posy-=1
end
-->8
-- main render loop
function _draw()
	cls(0)
	spr(jordan.sp,jordan.posx,jordan.posy)
	spr(mark.sp,mark.posx,mark.posy)
end
-->8
-- audio functions

function play_sound()
	sfx(5)
end
